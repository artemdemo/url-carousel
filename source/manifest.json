{
    "name": "URL carousel",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Gets information from Google.",
    "icons": {
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "permissions": [
        "tabs",
        "<all_urls>"
    ],
    "browser_action": {
        "default_icon": {
            "32": "images/icon32.png",
            "48": "images/icon48.png"
        },
        "default_title": "URL carousel",
        "default_popup": "popup.html"
    },
    "options_ui": {
        "page": "options.html",
        "chrome_style": false
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}
